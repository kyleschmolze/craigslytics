<div class="row-fluid" id="listing<%=listing.id%>">

  <div class="well span8 offset2">
    <div class="row-fluid">
      <h2><%= listing.address %></h2>
      <h4><%= listing.info["heading"] %></h4>
      <a href="<%= listing.info["external_url"] %>"> <%= listing.info["external_url"] %></a>
    </div>
    <div class="row-fluid">
      <div class="span3">
        <div style="text-align:center">
          <h5>Bedrooms: <%= listing.bedrooms %></h5>
          <h5>Price: $<%= listing.price %></h5>
          <% for tag in listing.tags do %>
            <span class="badge badge-info"><%= tag.name %></span>
          <% end %>
        </div>
      </div>
      <div class="span9">
        <%= listing.body %>
      </div>
    </div>
  </div>
</div>
