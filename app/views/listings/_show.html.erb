<div class="row-fluid" id="listing<%=listing.id%>">

  <div class="well span8 offset2">
    <div class="row-fluid">
      <h2><%= listing.address %></h2>
      <h4><%= listing.info["heading"] %></h4>
    </div>
    <div class="row-fluid">
      <div class="span3">
        <div>
          <h4>Bedrooms: <%= listing.bedrooms %></h4>
        </div>
        <div>
          <h4>Price: $<%= listing.price %></h4>
        </div>
      </div>
      <div class="span9">
        <%= listing.info["body"] %>
      </div>
    </div>
  </div>
</div>
